{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(admin4768932|bingo-2025)?",
      "dest": "/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}