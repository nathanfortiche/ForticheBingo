{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.ts"
    },
    {
      "src": "^/admin4768932/?$",
      "dest": "dist/public/index.html"
    },
    {
      "src": "^/bingo-2025/?$",
      "dest": "dist/public/index.html"
    },
    {
      "src": "^/static/(.*)",
      "dest": "dist/public/static/$1"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "dist/public/assets/$1"
    },
    {
      "src": ".*\\.(js|css|ico|png|jpg|svg|json)$",
      "dest": "dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}